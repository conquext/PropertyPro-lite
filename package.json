{
  "name": "propertypro-lite",
  "version": "1.0.0",
  "description": "Property Pro Lite is a platform where people can create and/or search properties for sale or rent.",
  "main": "app.js",
  "scripts": {
    "start": "DEBUG=dev node --require babel-register API/app.js && npm run sync",
    "start-dev": "DEBUG=dev node_modules/.bin/nodemon API/app.js --exec babel-node --",
    "docs": "node_modules/.bin/jsdoc -r  . -d docs -c API/jsdoc.json README.md",
    "swagger-docs": "node_modules/.bin/swagger-jsdoc -d swaggerDoc.js -o swagger.json",
    "dev-start": "nodemon --exec babel-node API/app.js",
    "lint": "eslint \"**/*.js\"",
    "lint:fix": "./node_modules/.bin/eslint . --fix || true",
    "test": "cross-env NODE_ENV=test node node_modules/.bin/_mocha --require babel-register API/test/*.js --timeout 45000 --exit -R spec",
    "test:db": "cross-env NODE_ENV=test npm run seeder && nyc mocha --require babel-register API/test/*.js --timeout 95000 --exit -R spec",
    "seeder": "DEBUG=dev babel-node ./API/db/seeder",
    "sync": "DEBUG=prod babel-node ./API/db/sync",
    "coveralls": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-lcov npm test",
    "coverage": "nyc npm run test",
    "heroku-postbuild": "pm2 start npm -- start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conquext/PropertyPro-lite.git"
  },
  "author": "Rasheed",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/conquext/PropertyPro-lite/issues"
  },
  "homepage": "https://github.com/conquext/PropertyPro-lite#readme",
  "dependencies": {
    "@types/express": "^4.17.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cassandra-map": "^0.1.7",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "chai-like": "^1.1.1",
    "chai-things": "^0.2.0",
    "cloudinary": "^1.11.0",
    "connect": "^3.7.0",
    "cordova": "^9.0.0",
    "debug": "^4.1.1",
    "dotenv": "^7.0.0",
    "esm": "^3.2.22",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.5",
    "express-list-endpoints": "^4.0.1",
    "express-validator": "^5.3.1",
    "forever": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "moment": "^2.24.0",
    "multer": "^1.4.1",
    "nodemailer": "^6.3.0",
    "pg": "^7.11.0",
    "pm2": "^3.5.1",
    "preq": "^0.5.9",
    "serve-static": "^1.14.1",
    "swagger-jsdoc": "^3.2.9",
    "swagger-test": "^0.7.3",
    "swagger-ui": "^4.1.2",
    "swagger-ui-express": "^4.0.7",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^12.0.10",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "coveralls": "^3.0.4",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jsdoc": "^3.6.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.11",
    "nyc": "^14.1.1"
  }
}
