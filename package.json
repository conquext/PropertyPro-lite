{
  "name": "propertypro-lite",
  "version": "1.0.0",
  "description": "Property Pro Lite is a platform where people can create and/or search properties for sale or rent.",
  "main": "app.js",
  "scripts": {
    "start": "node_modules/.bin/nodemon API/app.js --exec babel-node --",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./UI,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "serve": "node dist/app.js",
    "dev-start": "nodemon --exec babel-node API/app.js",
    "lint": "eslint \"**/*.js\"",
    "lint:fix": "./node_modules/.bin/eslint . --fix || true",
    "start:server": "babel-watch API/app.js",
    "test": "cross-env NODE_ENV=test nyc mocha --require @babel/register API/test/*.js --timeout 45000 --exit",
    "cover": "istanbul cover _mocha ",
    "coveralls": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-lcov npm test",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conquext/PropertyPro-lite.git"
  },
  "author": "Rasheed",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/conquext/PropertyPro-lite/issues"
  },
  "homepage": "https://github.com/conquext/PropertyPro-lite#readme",
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai-http": "^4.2.1",
    "cloudinary": "^1.11.0",
    "cordova": "^9.0.0",
    "dotenv": "^7.0.0",
    "esm": "^3.2.22",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@types/node": "^12.0.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.11",
    "nyc": "^14.0.0",
    "rimraf": "^2.6.3"
  }
}
